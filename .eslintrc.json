{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["warn", "single"],
    "no-prototype-builtins": ["off"],
    "no-undef": ["off"],
    "no-unused-vars": ["error"]
  }
}
